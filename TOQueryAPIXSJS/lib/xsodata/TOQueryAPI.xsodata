service {
    "db.models::TOQueryAPI_Header.TOHeader" as "TOHeader" key("Lgnum", "Tanum")
	   navigates ("TOItem" as "TOItem");

	"db.models::TOQueryAPI_Item.TOItem"
	  as "TOItem" key("Lgnum", "Tanum");

	association "TOItem" principal  "TOHeader"("Lgnum", "Tanum")
	multiplicity "1" dependent "TOItem"("Lgnum", "Tanum") multiplicity "*";
}
